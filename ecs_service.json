{
    "family": "my-task-family",
    "containerDefinitions": [
      {
        "name": "my-ecs-cluster",
        "image": "238026915774.dkr.ecr.us-east-1.amazonaws.com/demo_ecr_repo:1.0",
        "memory": 512,
        "cpu": 256,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ],
        "environment": [
          {
            "name": "NODE_ENV",
            "value": "dev"
          }
        ]
      }
    ]
  }
  